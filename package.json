  "jest": {
    "modulePaths": [
      "/shared/vendor/modules"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "jest-css-modules-transform",
      "\\.(png|gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js",
      "^~/(.*)$": "<rootDir>/src/$1",
      "^@config$": "<rootDir>/src/config/index.ts",
      "^~components/(.*)$": "<rootDir>/src/$1",
      "^~atoms/(.*)$": "<rootDir>/src/atoms/$1",
      "^~molecules/(.*)$": "<rootDir>/molecules//$1",
      "^~organisms/(.*)$": "<rootDir>/organisms//$1",
      "^~organisms.custom/(.*)$": "<rootDir>/organisms.custom//$1",
      "^~bases/(.*)$": "<rootDir>/src/bases/$1",
      "^~commons/(.*)$": "<rootDir>/commons//$1",
      "^~features/(.*)$": "<rootDir>/features//$1",
      "^~features.custom/(.*)$": "<rootDir>/features.custom//$1",
      "^~templates/(.*)$": "<rootDir>/templates//$1",
      "^~pages/(.*)$": "<rootDir>/src/pages/$1",
      "^~stores/(.*)$": "<rootDir>/src/stores/$1",
      "^~utils/(.*)$": "<rootDir>/src/utils/$1",
      "^@utils$": "<rootDir>/src/utils/index.ts",
      "^@hocs$": "<rootDir>/src/hocs/index.tsx",
      "^@hooks$": "<rootDir>/src/hooks/index.tsx",
      "^~svg/(.*)$": "<rootDir>/src/components/commons/svg/$1"
